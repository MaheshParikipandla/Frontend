sum([1,2,3,4,5,6,7,8],10);
function sum(array,targetsum)
{
    for(let i=0;i<array.length-1;i++)
    {
        for(let j=i;j<array.length;j++)
        {
            let sum=array[i]+array[j+1];
            if(sum===targetsum)
            {
                console.log('(',array[i],',',array[j+1],')');
            }
        }
    }
}
